{
  "name": "videomixpro",
  "version": "1.0.0",
  "description": "Video Mixer Pro - SaaS platform for automated video mixing and A/B testing",
  "main": "dist/index.js",
  "_moduleAliases": {
    "@": "dist",
    "@/controllers": "dist/controllers",
    "@/services": "dist/services",
    "@/models": "dist/models",
    "@/middleware": "dist/middleware",
    "@/routes": "dist/routes",
    "@/utils": "dist/utils",
    "@/workers": "dist/workers",
    "@/types": "dist/types"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "dev:setup": "npm run db:setup",
    "build": "tsc",
    "start": "node -r module-alias/register dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:setup": "prisma generate && prisma db push",
    "db:generate-schema": "node scripts/generate-schema.js",
    "db:switch-sqlite": "cp .env.sqlite .env && npm run db:generate-schema && npx prisma generate",
    "db:switch-postgres": "cp .env.postgres .env && npm run db:generate-schema && npx prisma generate",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "db:validate": "node scripts/validate-db.js",
    "queue:dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/workers/index.ts",
    "queue:prod": "node dist/workers/index.js",
    "db:init": "node scripts/init-prod-db.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup:dev": "node scripts/setup-dev.js",
    "setup:ffmpeg": "node scripts/download-ffmpeg.js",
    "test:ffmpeg": "ts-node -r tsconfig-paths/register scripts/test-ffmpeg.ts"
  },
  "keywords": [
    "video",
    "mixing",
    "saas",
    "automation",
    "a/b-testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "prisma": "^6.16.3",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/archiver": "^6.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.1",
    "@types/pdfkit": "^0.17.3",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^10.0.0",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1380.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "pdfkit": "^0.17.2",
    "stripe": "^18.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.0",
    "zod": "^3.21.0"
  }
}
